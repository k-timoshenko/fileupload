{
  "name": "t-kanstantsin/fileupload",
  "description": "File upload and store utilities",
  "type": "library",
  "keywords": [
    "file",
    "fileupload",
    "upload"
  ],
  "homepage": "https://github.com/t-kanstantsin/yii2-fileupload",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Konstantin Timoshenko",
      "email": "t.kanstantsin@gmail.com",
      "homepage": "https://github.com/t-kanstantsin",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "t-kanstantsin/yii2-flysystem": "^1.0",
    "guzzlehttp/guzzle": "^6.3.0",
    "imagine/imagine": "^0.7.1"
  },
  "require-dev": {
    "yiisoft/yii2": "^2.0.12",
    "bower-asset/jquery": "^3.2.1",
    "bower-asset/inputmask": "~3.3.5",
    "bower-asset/punycode": "~1.3.2",
    "bower-asset/yii2-pjax": "~2.0.7",

    "codeception/codeception": "^2.3",
    "codeception/verify": "~0.3.1",
    "fzaninotto/faker": "1.5.*",
    "ralouphie/mimey": "^1.0"
  },
  "suggest": {
    "fortawesome/font-awesome": "For file types without preview generator"
  },
  "autoload": {
    "psr-4": {
      "tkanstantsin\\fileupload\\": "src/file",
      "tkanstantsin\\yii2fileupload\\": "src/yii2"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
